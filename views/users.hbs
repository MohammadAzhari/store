<div class="alert alert-secondary" role="alert"><a href="/">back to home page</a></div>
<table class="table">
  <thead>
    <tr>
      <th scope="col">number</th>
      <th scope="col">users Email</th>
      <th scope="col">users id</th>
      <th scope="col"></th>
    </tr>
  </thead>
  {{#each arr as |a num|}}
  <tbody>
    <tr>
      <th scope="row"> #{{num}} </th>
      <td> {{this.email}} </td>
      <td>{{this._id}}</td>
      <td><form action="userdelete" method="post"><input type="hidden" value="{{this._id}}" name="del"><button type="submit" class="btn btn-danger">delete</button></form></td>
    </tr>
  </tbody>
  {{/each}}
</table>